{
    "name": "sandritsch91/yii2-webauthn",
    "description": "WebAuthn for Yii framework",
    "keywords": [
        "yii2",
        "webAuthn"
    ],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/Sandritsch91/yii2-webAuthn/issues",
        "source": "https://github.com/Sandritsch91/yii2-webAuthn"
    },
    "authors": [
        {
            "name": "Sandritsch91",
            "email": "sandritsch91@gmail.com"
        }
    ],
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "require": {
        "php": ">=8.2",
        "yiisoft/yii2": "^2.0.15",
        "web-auth/webauthn-lib": "^4.7.0"
    },
    "autoload": {
        "psr-4": {
            "sandritsch91\\yii2\\webauthn\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    }
}
